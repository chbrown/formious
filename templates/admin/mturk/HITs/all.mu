<ng-include src="'/templates/admin/mturk/nav.html'"></ng-include>
<main ng-controller="adminHITsCtrl">
  <h3>HITs</h3>

  <section ng-repeat="hit in hits" class="box">
    <h3>
      <a href="/admin/mturk/HITs/{{hit.HITId}}">
        {{hit.Title}}
      </a>
    </h3>
    <table class="keyval">
      <tr ng-repeat="(key, val) in hit">
        <td>{{key}}</td>
        <td>{{val}}</td>
      </tr>
    </table>
  </section>
</main>
