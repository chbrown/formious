{
  "name": "formious",
  "description": "Experimental experience server",
  "keywords": [
    "forms",
    "surveys"
  ],
  "version": "0.4.4",
  "homepage": "https://github.com/chbrown/formious",
  "repository": "git://github.com/chbrown/formious.git",
  "author": "Christopher Brown <io@henrian.com> (http://henrian.com)",
  "license": "MIT",
  "dependencies": {
    "angular": "1.4.3",
    "angular-resource": "1.4.3",
    "angular-ui-router": "0.2.15",
    "async": "2.6.0",
    "checkbox-sequence": "0.0.1",
    "cookiemonster": "1.1.0",
    "cookies": "chbrown/cookies#v1.0.0",
    "flow-copy": "0.1.2",
    "handlebars": "4.0.11",
    "httprequest": "0.1.3",
    "jszip": "^2.6.1",
    "lodash": "3.10.0",
    "loge": "1.0.5",
    "ng-upload": "0.1.0",
    "ngstorage": "0.3.7",
    "notify-ui": "0.2.0",
    "optimist": "0.6.1",
    "regex-router": "2.0.0",
    "send": "0.16.2",
    "sql-patch": "1.1.0",
    "sqlcmd-pg": "3.0.1",
    "streaming": "1.1.0",
    "sv": "0.4.2",
    "textarea": "0.2.0",
    "turk": "0.7.0",
    "urlobject": "0.0.2",
    "virtual-dom": "2.1.1",
    "xlsx": "0.12.3",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/handlebars": "^4.0.36",
    "@types/jszip": "^0.0.27",
    "@types/lodash": "^3.10.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "latest",
    "@types/optimist": "^0.0.29",
    "@types/pg": "^6.1.2",
    "@types/send": "^0.14.4",
    "@types/xmldom": "^0.1.29",
    "autoprefixer": "^7.1.5",
    "babel": "5.8.21",
    "babelify": "6.1.3",
    "browserify": "11.0.1",
    "clean-css": "^4.1.9",
    "clean-css-cli": "^4.1.10",
    "less": "^2.7.2",
    "mocha": "^5.0.1",
    "nodemon": "^1.17.1",
    "postcss-cli": "^4.1.1",
    "typescript": "^2.7.2",
    "watchify": "3.3.1"
  },
  "bin": {
    "formious": "bin/formious"
  },
  "scripts": {
    "prepare": "tsc",
    "prepack-css": "lessc ui/site.less | postcss --use autoprefixer | cleancss --format breaks:afterRuleEnds=on -o ui/site.css",
    "prepack-js": "browserify ui/build.js -t babelify | closure-compiler --angular_pass --language_in ECMASCRIPT5 --warning_level QUIET > ui/build/bundle.js",
    "prepack": "npm run prepack-css && npm run prepack-js",
    "dev": "tsc --watch & watchify ui/build.js -t babelify -o ui/build/bundle.js -v & PORT=1451 nodemon bin/formious",
    "start": "node bin/formious",
    "test": "mocha",
    "clean": "tsc -d --listEmittedFiles | grep '^TSFILE: ' | sed 's/^TSFILE: //' | xargs rm -v"
  }
}
