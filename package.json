    "xpath": "^0.0.27"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/handlebars": "^4.0.36",
    "@types/jszip": "^0.0.27",
    "@types/lodash": "^3.10.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "^9.6.1",
    "@types/optimist": "^0.0.29",
    "@types/pg": "^6.1.2",
    "@types/send": "^0.14.4",
    "@types/xmldom": "^0.1.29",
    "ajv": "^6.2.1",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-angularjs-annotate": "^0.8.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-css": "^4.1.9",
    "clean-css-cli": "^4.1.10",
    "less": "^3.0.1",
    "mocha": "^5.0.1",
    "nodemon": "^1.17.3",
    "postcss-cli": "^5.0.0",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.10"
  },
  "bin": {
    "formious": "bin/formious"
  },
  "scripts": {
    "prepare": "tsc",
    "prepack-css": "lessc ui/site.less | postcss --use autoprefixer | cleancss --format breaks:afterRuleEnds=on -o ui/site.css",
    "prepack-js": "NODE_ENV=production webpack",
    "prepack": "npm run prepack-css && npm run prepack-js",
    "dev": "tsc --watch & webpack --watch & PORT=1451 nodemon bin/formious",
    "start": "node bin/formious",
    "test": "mocha",
    "clean": "tsc -d --listEmittedFiles | grep '^TSFILE: ' | sed 's/^TSFILE: //' | xargs rm -v"
  }
}
